{
  "name": "@xagi/ai-template-react-next-app",
  "version": "1.2.2",
  "description": "Modern Next.js application with Pages mode, unstyled components and XAGI AI coding integration",
  "private": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "keywords": [
    "nextjs",
    "react",
    "typescript",
    "tailwindcss",
    "shadcn-ui",
    "ai-template",
    "xagi",
    "mcp",
    "ai-agent",
    "template",
    "frontend",
    "modern"
  ],
  "author": "XAGI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xagi/ai-template-react-next-app.git"
  },
  "bugs": {
    "url": "https://github.com/xagi/ai-template-react-next-app/issues"
  },
  "homepage": "https://github.com/xagi/ai-template-react-next-app#readme",
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "build:production": "NODE_ENV=production next build",
    "build:analyze": "ANALYZE=true next build",
    "build:debug": "NODE_OPTIONS='--inspect' next build",
    "start": "next start",
    "start:production": "NODE_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "clean": "rm -rf .next out",
    "prebuild": "npm run clean",
    "postbuild": "next-sitemap",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "ai:init": "node scripts/ai-agent-cli.js init",
    "ai:component": "node scripts/ai-agent-cli.js component",
    "ai:page": "node scripts/ai-agent-cli.js page",
    "ai:validate": "node scripts/ai-agent-cli.js validate",
    "ai:validate:fix": "node scripts/ai-agent-cli.js validate --fix",
    "ai:claude": "node scripts/ai-agent-cli.js claude",
    "ai:codex": "node scripts/ai-agent-cli.js codex",
    "ai:optimize": "node scripts/ai-agent-cli.js optimize",
    "ai:monitor": "node scripts/ai-performance-monitor.js",
    "ai:report": "node scripts/ai-performance-monitor.js report",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "node scripts/check-package-manager.js",
    "check:deps": "pnpm audit",
    "check:outdated": "pnpm outdated",
    "update:deps": "pnpm update --interactive",
    "clean:deps": "rm -rf node_modules pnpm-lock.yaml && pnpm install",
    "test:unit": "vitest run --reporter=verbose",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:a11y": "vitest run --config vitest.a11y.config.ts",
    "test:performance": "vitest run --config vitest.performance.config.ts",
    "lighthouse:ci": "lhci autorun",
    "analyze": "ANALYZE=true next build",
    "changelog": "node scripts/version-manager.js changelog",
    "version:current": "node scripts/version-manager.js current",
    "version:release": "node scripts/version-manager.js release",
    "version:major": "node scripts/version-manager.js release major",
    "version:minor": "node scripts/version-manager.js release minor",
    "version:patch": "node scripts/version-manager.js release patch",
    "deploy:dev": "node scripts/deploy.js development",
    "deploy:staging": "node scripts/deploy.js staging",
    "deploy:prod": "node scripts/deploy.js production",
    "monitoring:setup": "node scripts/monitoring-setup.js",
    "build:package": "npm run build && npm pack",
    "template:init": "node scripts/template-publisher.js init",
    "template:validate": "node scripts/template-publisher.js validate",
    "template:package": "node scripts/template-publisher.js package",
    "template:publish": "node scripts/template-publisher.js publish",
    "template:publish:dry": "node scripts/template-publisher.js publish --dry-run",
    "template:list": "node scripts/template-publisher.js list",
    "monitor:start": "node -e \"require('./src/lib/maintenance-system.ts').maintenanceSystem.startAutoUpdate()\"",
    "monitor:stop": "node -e \"require('./src/lib/maintenance-system.ts').maintenanceSystem.stop()\"",
    "monitor:health": "curl -f http://localhost:3000/api/maintenance?action=health",
    "monitor:optimize": "curl -X GET http://localhost:3000/api/maintenance?action=optimize",
    "validate": "node scripts/run-validation.js",
    "validate:quick": "npm run test:unit && npm run lint && npm run type-check",
    "validate:full": "node scripts/run-validation.js",
    "validate:final": "node scripts/final-validation.js",
    "release:prepare": "node scripts/prepare-release.js",
    "release:validate": "npm run validate:final && npm run release:prepare",
    "test:ai": "vitest run src/test/comprehensive-validation.test.ts",
    "test:validation": "vitest run src/test/performance-validation.test.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.59.0",
    "@tanstack/react-query-devtools": "^5.59.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "next": "^14.2.15",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.63.0",
    "react-syntax-highlighter": "^15.6.6",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@axe-core/cli": "^4.8.3",
    "@eslint/eslintrc": "^3",
    "@lhci/cli": "^0.12.0",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.8.3",
    "eslint": "^8",
    "eslint-config-next": "^14.2.15",
    "jest-axe": "^8.0.0",
    "jsdom": "^23.0.1",
    "lighthouse": "^11.4.0",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.5.6",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^3.4.0",
    "typescript": "^5",
    "vite": "^5.4.20",
    "vitest": "^1.0.4",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "browserslist": [
    "Chrome >= 87",
    "Firefox >= 78",
    "Safari >= 14",
    "Edge >= 87"
  ],
  "files": [
    "src/",
    "public/",
    "components.json",
    "next.config.js",
    "next-sitemap.config.js",
    "tailwind.config.ts",
    "tsconfig.json",
    "postcss.config.mjs",
    "eslint.config.mjs",
    "vitest.config.ts",
    "vitest.*.config.ts",
    "scripts/",
    "docs/",
    "template.json",
    "template-init.js",
    "README.md",
    "AGENTS.md",
    "BUILD_OPTIMIZATION.md",
    "DEPLOYMENT.md",
    "MIGRATION_GUIDE.md",
    "RELEASE_NOTES.md",
    "SUPPORT_PLAN.md",
    "*.ai",
    "**/*.ai"
  ],
  "template": {
    "name": "react-next-app",
    "version": "1.2.0",
    "category": "frontend",
    "description": "Modern Next.js application with Pages mode, unstyled components and XAGI AI coding integration",
    "features": [
      "Next.js 14支持",
      "Pages模式",
      "无样式组件库",
      "XAGI智能编码"
    ],
    "techStack": [
      "Next.js 14",
      "React 18",
      "TypeScript",
      "Tailwind CSS",
      "shadcn/ui",
      "Radix UI",
      "React Hook Form",
      "Zod",
      "TanStack Query",
      "Vitest",
      "pnpm"
    ],
    "aiIntegration": {
      "supported": true,
      "agents": [
        "claude",
        "codex",
        "cursor",
        "windsurf"
      ],
      "features": [
        "AI组件生成",
        "智能代码优化",
        "自动化测试生成",
        "性能监控",
        "代码质量检查"
      ]
    },
    "initCommand": "npx @xagi/ai-template-react-next-app@latest",
    "postInstall": "node template-init.js"
  }
}