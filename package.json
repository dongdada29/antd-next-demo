{
  "name": "antd-next-demo",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "build:production": "NODE_ENV=production next build",
    "build:analyze": "ANALYZE=true next build",
    "build:debug": "NODE_OPTIONS='--inspect' next build",
    "start": "next start",
    "start:production": "NODE_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "clean": "rm -rf .next out",
    "prebuild": "npm run clean && npm run type-check",
    "postbuild": "next-sitemap",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "ai:init": "node scripts/ai-agent-cli.js init",
    "ai:component": "node scripts/ai-agent-cli.js component",
    "ai:page": "node scripts/ai-agent-cli.js page",
    "ai:validate": "node scripts/ai-agent-cli.js validate",
    "ai:validate:fix": "node scripts/ai-agent-cli.js validate --fix",
    "ai:claude": "node scripts/ai-agent-cli.js claude",
    "ai:codex": "node scripts/ai-agent-cli.js codex",
    "ai:optimize": "node scripts/ai-agent-cli.js optimize",
    "ai:monitor": "node scripts/ai-performance-monitor.js",
    "ai:report": "node scripts/ai-performance-monitor.js report",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "node scripts/check-package-manager.js",
    "check:deps": "pnpm audit",
    "check:outdated": "pnpm outdated",
    "update:deps": "pnpm update --interactive",
    "clean:deps": "rm -rf node_modules pnpm-lock.yaml && pnpm install"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.59.0",
    "@tanstack/react-query-devtools": "^5.59.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "next": "^14.2.15",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.63.0",
    "react-syntax-highlighter": "^15.6.6",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^8",
    "eslint-config-next": "^14.2.15",
    "jsdom": "^23.0.1",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.5.6",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.13",
    "typescript": "^5",
    "vitest": "^1.0.4",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "browserslist": [
    "Chrome >= 87",
    "Firefox >= 78",
    "Safari >= 14",
    "Edge >= 87"
  ]
}